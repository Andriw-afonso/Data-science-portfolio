CREATE DATABASE `employees` /*!40100 DEFAULT CHARACTER SET utf8mb4 COLLATE utf8mb4_0900_ai_ci */ /*!80016 DEFAULT ENCRYPTION='N' */;

USE employees;

DROP TABLE IF EXISTS accident;
CREATE TABLE `accident` (
  `emp_no` bigint DEFAULT NULL,
  `Event Description` text,
  `Event Keywords` text,
  `Human Factor` text
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_0900_ai_ci;

DROP TABLE IF EXISTS employees;
CREATE TABLE `employees` (
  `emp_no` bigint DEFAULT NULL,
  `birth_date` datetime DEFAULT NULL,
  `first_name` text,
  `last_name` text,
  `gender` text,
  `department` text,
  `left` bigint DEFAULT NULL,
  `hire_date` text
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_0900_ai_ci;

DROP TABLE IF EXISTS projects;
CREATE TABLE `projects` (
  `PROJECT_ID` bigint DEFAULT NULL,
  `PROJECT NAME` text,
  `PROJECT SCOPE` text,
  `TYPE` text,
  `STATUS` text,
  `% COMPLETE` text,
  `PROJECT BUDGET` text
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_0900_ai_ci;

DROP TABLE IF EXISTS projects_emp;
CREATE TABLE `projects_emp` (
  `EMP_ID` bigint DEFAULT NULL,
  `PROJECT_ID` bigint DEFAULT NULL
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_0900_ai_ci;

DROP TABLE IF EXISTS salaries;
CREATE TABLE `salaries` (
  `EMP_ID` bigint DEFAULT NULL,
  `SALARY` text
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_0900_ai_ci;
